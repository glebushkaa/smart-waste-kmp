CREATE TABLE IF NOT EXISTS rubbish (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    emoji TEXT NOT NULL,
    count INTEGER NOT NULL
);

getAllRubbishes: SELECT * FROM rubbish WHERE count > 0;
insertRubbish: INSERT OR REPLACE INTO rubbish(id,name, emoji,count) VALUES(?,?,?,?);
updateRubbishCount: UPDATE rubbish SET count = ? WHERE id = ?;